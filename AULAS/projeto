#include <stdio.h>
#include <string.h>
int main() {
char funcionario[1][50];
char cliente[1][50];
char senhasf[1][20];
char senhasc[1][20];
char pessoas;
int i;
cliente==('connor');
senhasc==('mulher');
funcionario==('stefanny');
senhasf==('ladygp');
    printf("digite se for cliente(c) ou funcionario(f): \n");
    scanf("%c",&pessoas);
if (pessoas=='c'){
    printf("vc escolheu cliente\n");
printf("Digite seu nome: ");
fgets(cliente[i], 50, stdin);
cliente[i][strcspn(cliente[i], "\n")] = 0;
printf("Digite a senha: ");
fgets(senhasc[i], 20, stdin);
printf("\n");
}
else if(pessoas=='f') {
    printf("vc escolheu funcionario");
    for(i = 0; i < 2; i++) {
printf("Cadastro da funcionario %d\n", i + 1);
printf("Digite o nome: ");
fgets(funcionario[i], 50, stdin);
funcionario[i][strcspn(funcionario[i], "\n")] = 0;
printf("Digite a senha: ");
fgets(senhasf[i], 20, stdin);
printf("\n");
}
}
return 0;
}
